{
  "name": "animated-value",
  "version": "0.1.0",
  "description": "An imperative animation API for declarative UI renderers",
  "main": "src/index.js",
  "repository": "git@github.com:thesephist/animated-value.git",
  "author": "Linus Lee <linus@thesephist.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js && cp dist/index.min.js demo/",
    "build:watch": "webpack --config webpack.config.js --watch",
    "test:size": "bundlesize",
    "prepublishOnly": "webpack --config webpack.config.js",
    "deploy:demo": "surge -p demo/ --domain animated-value.surge.sh"
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "bezier-easing": "^2.1.0"
  },
  "devDependencies": {
    "bundlesize": "^0.17.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1"
  },
  "bundlesize": [
    {
      "path": "dist/*.js",
      "threshold": "2.5kB"
    }
  ],
  "keywords": [
    "animate",
    "animation",
    "easing",
    "bezier",
    "component",
    "transition",
    "torus",
    "react"
  ]
}
